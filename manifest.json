{
    "manifest_version": 3,
    "name": "codepiep_img_extension",
    "version": "1.0.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "deafult_title": "SRK Fan"
    },
    "permissions": ["tabs"]
  }